;; fib - harmless-tech
;; Computes the numbers of the fib sequence up to the 150th fibonacci number.
:START::FIB       ; Right now this would generate a nop instruction, but in the future it should link to the instruction below it.
mov r0 usize(150) ; Counter
mov r5 u128(0)
mov r1 u128(1)
:FIB::LOOP nop    ; Loop part
call (println)
cpy r2 r5
op + r5 r1
cpy r1 r2
mov r3 usize(0)
op -- r0 r255
op != r3 r0
jmp r3 add(:FIB::LOOP) ; Can load address directly into a Jmp instruction.
exit i32(0)
